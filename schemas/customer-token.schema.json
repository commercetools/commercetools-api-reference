  "title":"Customer Token",
  "required": ["id", "customerId", "createdAt", "lastModifiedAt", "expiresAt", "value"],

  "properties": {
    "id": {
      "$ref": "id.schema.json"
    },
    "customerId": {
      "type": "string"
    },
    "createdAt":{
      "$ref":"schemas/created-at.schema.json"
    },
    "lastModifiedAt":{
      "$ref":"schemas/last-modified-at.schema.json"
    },
    "expiresAt": {
      "type": "string"
    },
    "value": {
      "type": "string"
    }
  }
}
