#%RAML 1.0 DataType
(package): Cart
displayName: CartOrigin
type: string
description: |
  Indicates who created the Cart.
enum:
  - Customer
  - Merchant
  - Quote
(enumDescriptions):
  Customer: |
    Cart was created by a Customer.

    This is the default value.
  Merchant: |
    Cart was created by a merchant on behalf of a Customer.
  Quote: |
    Cart was created as part of a Quote.
