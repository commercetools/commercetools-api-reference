#%RAML 1.0 DataType
(package): Order
(docs-uri): https://docs.commercetools.com/api/projects/carts#itemstate
displayName: ItemState
type: object
properties:
  quantity:
    type: number
    format: int64
    description: |
      Number of Line Items or Custom Line Items in this State.
  state:
    type: StateReference
    description: |
      State of the Line Items or Custom Line Items in a custom workflow.
