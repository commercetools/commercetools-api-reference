(package): Product
type: ProductUpdateAction
displayName: ProductSetKeyAction
discriminatorValue: setKey
example: !include ../../../examples/Product/ProductSetKeyAction.json
properties:
  key?:
    type: string
    description: |
      Value to set. If empty, any existing value will be removed.

      To update a Product using the [Import API](/../import-export/product), the Product `key` must match the pattern `^[A-Za-z0-9_-]{2,256}$`.
