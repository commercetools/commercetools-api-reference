#%RAML 1.0 DataType
(package): Error
type: GraphQLErrorObject
displayName: GraphQLDuplicateFieldError
discriminatorValue: DuplicateField
description: |
  Returned when a field value conflicts with an existing value causing a duplicate.
properties:
  code:
    type: string
  field:
    type: string
    description: |
      Name of the conflicting field.
  duplicateValue:
    type: any
    description: |
      Conflicting duplicate value.
