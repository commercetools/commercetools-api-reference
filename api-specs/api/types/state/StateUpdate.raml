#%RAML 1.0 DataType
(package): State
type: object
displayName: StateUpdate
(java-extends): 'com.commercetools.api.models.ResourceUpdate<StateUpdate, StateUpdateAction, StateUpdateBuilder>'
properties:
  version:
    type: number
    format: int64
    description: |
      Expected version of the State on which the changes should be applied. If the expected version does not match the actual version, a [409 Conflict](/../api/errors#409-conflict) will be returned.
  actions:
    type: array
    items: StateUpdateAction
    description: |
      Update actions to be performed on the State.
