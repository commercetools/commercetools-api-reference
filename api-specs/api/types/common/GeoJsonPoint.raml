#%RAML 1.0 DataType
(package): Common
displayName: GeoJsonPoint
type: GeoJson
discriminatorValue: Point
properties:
  coordinates:
    type: array
    items: number
    description: |
      Longitude (stored on index `[0]`) and latitude (stored on index `[1]`) of the [Point](https://datatracker.ietf.org/doc/html/rfc7946#section-3.1.2).
