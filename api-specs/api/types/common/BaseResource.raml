#%RAML 1.0 DataType
(package): Common
displayName: BaseResource
type: object
(expandable): true
(abstract): true
(ignoreValidators): [PolymorphicSubtypesRule]
properties:
  id:
    (identifier): true
    type: string
  version:
    type: number
    format: int64
  createdAt:
    type: datetime
  lastModifiedAt:
    type: datetime
