#%RAML 1.0 DataType
(package): AttributeGroup
displayName: AttributeGroup
(updateType): AttributeGroupUpdate
type: BaseResource
properties:
  id:
    (identifier): true
    type: string
    description: |
      Platform-generated unique identifier of the AttributeGroup.
  version:
    type: number
    format: int64
    description: |
      Current version of the AttributeGroup.
  createdAt:
    type: datetime
    description: |
      Date and time (UTC) the AttributeGroup was initially created.
  lastModifiedAt:
    type: datetime
    description: |
      Date and time (UTC) the AttributeGroup was last updated.
  lastModifiedBy?:
    type: LastModifiedBy
    (beta): true
    description: |
      IDs and references that last modified the AttributeGroup.
  createdBy?:
    type: CreatedBy
    (beta): true
    description: |
      IDs and references that created the AttributeGroup.
  name:
    type: LocalizedString
    description: |
      Name of the AttributeGroup.
  description?:
    type: LocalizedString
    description: |
      Description of the AttributeGroup.
  attributes:
    type: AttributeReference[]
    description: |
      [Attributes](ctp:api:type:AttributeDefinition) with unique values.
  key?:
    type: string
    description: |
      User-defined unique identifier of the AttributeGroup.
    pattern: ^[A-Za-z0-9_-]+$
    minLength: 2
    maxLength: 256
