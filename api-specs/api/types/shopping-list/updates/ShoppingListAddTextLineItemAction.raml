#%RAML 1.0 DataType
(package): ShoppingList
(docs-uri): https://docs.commercetools.com/api/projects/shoppingLists#add-textlineitem
type: ShoppingListUpdateAction
displayName: ShoppingListAddTextLineItemAction
discriminatorValue: addTextLineItem
example: !include ../../../examples/ShoppingList/ShoppingListAddTextLineItemAction.json
properties:
  name:
    type: LocalizedString
    description: |
      Name of the TextLineItem.
  description?:
    type: LocalizedString
    description: |
      Description of the TextLineItem.
  quantity?:
    type: number
    format: int64
    default: 1
