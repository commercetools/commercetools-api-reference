#%RAML 1.0 DataType
(package): Common
type: object
displayName: ItemShippingDetails
description:
properties:
  targets:
    type: array
    items:
      type: ItemShippingTarget
  valid:
    type: boolean
    description:
      true if the quantity of the (custom) line item is equal to the sum of the
      sub-quantities in `targets`, `false` otherwise. A cart cannot be ordered
      when the value is `false`. The error InvalidItemShippingDetails will be
      triggered.
