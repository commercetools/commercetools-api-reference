#%RAML 1.0 DataType
(package): Common
type: object
displayName: Location
description: A geographical location representing a country and optionally a state within this country.  A location can only be assigned to one Zone.
properties:
  country:
    type: CountryCode
    description: Country code of the geographic location.
  state?:
    type: string
    description: State within the country.
