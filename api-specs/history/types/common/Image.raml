#%RAML 1.0 DataType
(package): Common
displayName: Image
type: object
properties:
  url:
    type: string
    description: |
      URL of the image in its original size that must be unique within a single [ProductVariant](ctp:api:type:ProductVariant).
  dimensions:
    type: ImageDimensions
    description: |
      Dimensions of the original image.
  label?:
    type: string
    description: |
      Custom label for the image.
