#%RAML 1.0 DataType
(package): Common
type: object
displayName: ShippingRate
description:
properties:
  price:
    type: Money
  freeAbove:
    type: Money
  isMatching:
    type: boolean
    description: Only appears in response to requests for shipping methods by cart or location to mark this shipping rate as one that matches the cart or location.
  tiers:
    type: array
    items:
      type: ShippingRatePriceTier
