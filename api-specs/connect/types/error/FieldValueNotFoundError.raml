#%RAML 1.0 DataType
(annotations.package): error
type: ErrorObject
displayName: FieldValueNotFoundError
discriminatorValue: FieldValueNotFound
description: |
  Returned when a value for a field is not found.
properties:
  code:
    type: string
  message:
    type: string
    description: |
      `"The value $value for the $field field was not found"`
  field:
    type: string
    description: |
      Name of the field.
  value:
    type: any
    description: |
      Conflicting value.
