#%RAML 1.0 DataType
(annotations.package): Deployment
type: object
displayName: DeploymentUpdate
properties:
  version:
    type: number
    format: int64
    description: |
      Expected version of the Deployment on which the changes apply.
  actions:
    type: array
    items: DeploymentUpdateAction
    description: |
      Update actions to be performed on the Deployment.
