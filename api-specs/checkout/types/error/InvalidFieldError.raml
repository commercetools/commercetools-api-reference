#%RAML 1.0 DataType
(annotations.package): Error
(annotations.beta): true
type: ErrorObject
displayName: InvalidFieldError
discriminatorValue: InvalidField
description: |
  Returned when a field has an invalid value.
properties:
  code:
    type: string
  message:
    type: string
    description: |
      `"The value $invalidValue is not valid for field $field."`
  field:
    type: string
    description: |
      Name of the field with the invalid value.
  invalidValue:
    type: any
    description: |
      Value invalid for the field.
  allowedValues?:
    type: array
    items: string
    description: |
      Fixed set of allowed values for the field, if any.
