#%RAML 1.0 DataType
(annotations.package): Transaction
(annotations.beta): true
displayName: TransactionError
description: |
  A single error on the [Transaction](/transactions-api#transaction). Multiple errors may be included in the [Transaction Status](/transactions-api#transactionstatus).
properties:
  code:
    type: string
    description: |
      Error identifier.
  message:
    type: string
    description: |
      Plain text description of the cause of the error.
