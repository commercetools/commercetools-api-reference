#%RAML 1.0 DataType
(annotations.package): Common
type: object
displayName: LocalizedUrl
(annotations.asMap):
  key: string
  value: string
description: |
  A localized URL that provides different URL values for different locales.
properties:
  /^https?:\/\/[^\s/$.?#].[^\s]*$/:
    type: string
    description: |
      URL for the specific [locale](#locale).
example:
  en: 'https://example.com/en'
  de: 'https://example.com/de'
