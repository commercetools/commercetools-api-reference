#%RAML 1.0 DataType
(annotations.package): Sessions
type: object
displayName: UserToken
example: !include ../../examples/sessions/user-token.json
properties:
  token:
    type: string
    description: |
      The generated token for a store user with a given role.
  permissions:
    type: InStorePermission[]
    description: |
      The permissions the issued token has.
