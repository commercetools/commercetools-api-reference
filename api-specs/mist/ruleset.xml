<?xml version="1.0" encoding="UTF-8" ?>
<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/commercetools/rmf-codegen/main/ctp-validators/src/main/resources/ruleset.xsd">
    <name>default</name>
    <apply set="default"/>
    <rules>
        <rule>
            <name>com.commercetools.rmf.validators.ResourcePluralRule</name>
            <options>
                <option type="exclude">cash</option>
                <option type="exclude">configuration</option>
                <option type="exclude">device</option>
                <option type="exclude">location</option>
                <option type="exclude">location_status</option>
                <option type="exclude">payment</option>
                <option type="exclude">refund</option>
                <option type="exclude">role</option>
                <option type="exclude">session</option>
                <option type="exclude">user</option>
                <option type="exclude">workstation</option>
            </options>
        </rule>
        <rule>
            <name>com.commercetools.rmf.validators.CamelCaseRule</name>
            <options>
                <option type="exclude">__v</option>
                <option type="exclude">_id</option>
                <option type="exclude">account_type</option>
                <option type="exclude">address_line_1</option>
                <option type="exclude">address_line_2</option>
                <option type="exclude">alternate_currencies</option>
                <option type="exclude">application_preferred_name</option>
                <option type="exclude">authorization_response_code</option>
                <option type="exclude">callback_url</option>
                <option type="exclude">cancel_url</option>
                <option type="exclude">card_issuer</option>
                <option type="exclude">card_number</option>
                <option type="exclude">card_payment_provider</option>
                <option type="exclude">card_type</option>
                <option type="exclude">cart_origin</option>
                <option type="exclude">cart_total</option>
                <option type="exclude">citcon_token</option>
                <option type="exclude">client_name</option>
                <option type="exclude">created_at</option>
                <option type="exclude">dedicated_file_name</option>
                <option type="exclude">default_cashdrawer</option>
                <option type="exclude">default_PED</option>
                <option type="exclude">default_printer</option>
                <option type="exclude">default_scanner</option>
                <option type="exclude">device_id</option>
                <option type="exclude">extended_price</option>
                <option type="exclude">gift_card_processor</option>
                <option type="exclude">item_id</option>
                <option type="exclude">location_id</option>
                <option type="exclude">location_name</option>
                <option type="exclude">location_status</option>
                <option type="exclude">merchant_id</option>
                <option type="exclude">onscreen_keypad</option>
                <option type="exclude">postal_code</option>
                <option type="exclude">price_modifiers</option>
                <option type="exclude">processor_id</option>
                <option type="exclude">refund_line_items</option>
                <option type="exclude">safe_id</option>
                <option type="exclude">supported_languages</option>
                <option type="exclude">tax_code</option>
                <option type="exclude">tax_included_in_price</option>
                <option type="exclude">tax_rate</option>
                <option type="exclude">tax_summary</option>
                <option type="exclude">telephone_number</option>
                <option type="exclude">tenant_id</option>
                <option type="exclude">tenant_label</option>
                <option type="exclude">tender_line_items</option>
                <option type="exclude">tender_type</option>
                <option type="exclude">terminal_id</option>
                <option type="exclude">terminal_verification_results</option>
                <option type="exclude">total_gross</option>
                <option type="exclude">total_net</option>
                <option type="exclude">total_saved</option>
                <option type="exclude">total_tax</option>
                <option type="exclude">transaction_id</option>
                <option type="exclude">transaction_line_items</option>
                <option type="exclude">transaction_status_information</option>
                <option type="exclude">updated_at</option>
                <option type="exclude">user_id</option>
                <option type="exclude">wallet_processor</option>
                <option type="exclude">workstation_id</option>
            </options>
        </rule>
        <rule>
            <name>com.commercetools.rmf.validators.StringPropertySingularRule</name>
            <options>
                <option type="exclude">location_status</option>
                <option type="exclude">parameters</option>
                <option type="exclude">refund_line_items</option>
                <option type="exclude">status</option>
                <option type="exclude">templates</option>
                <option type="exclude">tender_line_items</option>
                <option type="exclude">terminal_verification_results</option>
            </options>
        </rule>
        <rule>
            <name>com.commercetools.rmf.validators.PropertyPluralRule</name>
            <options>
                <option type="exclude">tax_summary</option>
            </options>
        </rule>
    </rules>
</ruleset>
