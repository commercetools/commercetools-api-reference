#%RAML 1.0 Library
usage: Annotations test types.

uses:
  annotations: ../annotation-types/annotations.raml

types:
  AnnotationsTestType:
    properties:
      isBetaType:
        description: Beta property. If true, a "beta" flag should appear beside the type name.
        type: string
        (annotations.beta): true
      isOptionalBetaType?:
        description: Beta property. If true, a "beta" flag should appear beside the type name.
        type: string
        (annotations.beta): true
      isDeprecatedType:
        description: Deprecated property. THIS MUST NOT BE RENDERED AT ALL (=hidden in documentation, it just stays there to keep SDKs backwards compatible).
        type: string
        (annotations.deprecated): true

  AdditionalInfoTestType:
    properties:
      additionalInfoProperties:
        type: number
        minimum: 3600
        maximum: 604800
        default: 172800
        description: Additional info property. Added to test the additional info annotation.
      optionalBoolean?:
        type: boolean
        description: |
          Optional field with a default value.
        default: true
      minMaxItems:
        type: string[]
        minItems: 1
        maxItems: 10
        description: |
          Array with minimum and maximum items.
      patternField:
        type: string
        description: |
          String pattern field.
        minLength: 2
        maxLength: 256
        pattern: ^[A-Za-z0-9_-]+$

  SortingTestType:
    properties:
      fifthProperty:
        type: string
        description: |
          Fifth property in the sorting test type.
      thirdProperty:
        type: string
        description: |
          Third property in the sorting test type.
      fourthProperty:
        type: string
        description: |
          Fourth property in the sorting test type.
      firstProperty:
        type: string
        description: |
          First property in the sorting test type.
      secondProperty:
        type: string
        description: |
          Second property in the sorting test type.
